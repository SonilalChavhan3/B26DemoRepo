<div class="container">
    <div class="row justify-content-center">
        <div class="col-sm-10 col-md-6 col-lg-4">
            <div class="card shadow-lg text-center rounded-4">
                <div class="card-body p-4">

                    <!-- Profile Image -->
                    <img src="@Url.Content("~/image/1000051771.jpg")"
                         alt="User Image"
                         class="rounded-circle profile-img mb-3">

                    <!-- Name -->
                    <h4 class="card-title fw-bold">Sonilal Chavhan</h4>
                    <p class="text-muted mb-3">Software Developer</p>

                    <!-- Profile Details -->
                    <ul class="list-group list-group-flush text-start">
                        <li class="list-group-item">
                            <strong>Age:</strong> 28
                        </li>
                        <li class="list-group-item">
                            <strong>Gender:</strong> Male
                        </li>
                        <li class="list-group-item">
                            <strong>City:</strong> Pune, India
                        </li>
                    </ul>

                    <!-- Action Button -->
                    <div class="mt-4">
                        <a asp-controller="Home" asp-action="SonilalChavhan" class="btn btn-primary w-100 rounded-pill">
                            View Full Profile
                        </a>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>


